class Skatman {

    private String myName;
    private int myAge;

    public Skatman(aName, anAge) {
        myName = aName;
        myAge = anAge;
    }

    public static void greet(String aName) {
        theGreeting = "Hello there, " + aName + "!";
        System.out.println(theGreeting);
    }
